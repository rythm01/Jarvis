{
    "functions": {
        "api": {
            "runtime": "python3.8"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/app.py"
        }
    ],
    "builds": [
        {
            "src": "app.py",
            "use": "@now/streamlit"
        }
    ]
}